{  
    "invocation":"my test app",
    "intents":[
        {
			"name":"RepeatIntent",
			"alexa": {
				"name": "AMAZON.RepeatIntent"
			},
			"phrases":[  
                "repeat",
                "could you repeat that",
                "what",
				"say that again"
            ]
		}, 
        {  
            "name":"HelloWorldIntent",
            "phrases":[  
                "hello",
                "say hello",
                "say hello world"
            ]
        },
        {  
            "name":"MyNameIsIntent",
            "phrases":[  
                "{name}",
                "my name is {name}",
                "i am {name}",
                "you can call me {name}"
            ],
            "inputs":[  
                {  
                    "name":"name",
                    "type":{  
                        "alexa":"AMAZON.US_FIRST_NAME",
                        "dialogflow":"@sys.given-name"
                    }
                }
            ]
        }
    ]
}